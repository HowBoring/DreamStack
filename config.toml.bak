baseURL = "https://howboring.github.io/"
title = "InitialiZ"
defaultContentLanguage = "en"
themesDir = "themes"
theme = "zozo"
canonifyURLs = true
enableEmoji = true
enableGitInfo = true
hasCJKLanguage = true
enableRobotsTXT = true
disableRSS = false
disableSitemap = false
disable404 = false
disableHugoGeneratorInject = false

[params]
description = "Code as Poetry."
homeSubtitle = "Code as Poetry."
subTitle = "Code as Poetry."                                       # site's subTitle              # 网站二级标题
footerSlogan = "My spiritual home"                                   # site's footer slogan         # 网站页脚标语
keywords = ["Python", "C++", "Julia", "Deep Learning"]
enableMathJax = true                                                 # enable mathjax               # 是否使用mathjax（数学公式）
enableSummary = true                                                 # display the article summary  # 是否显示文章摘要
mainSections = ["posts","post"]

[[params.social]]
name = "email"
url = "mailto:code7thday@outlook.com"

